    assertEquals(b_int(-16256), b_byte(Byte.MAX_VALUE) * p_byte(Byte.MIN_VALUE))

    assertEquals(b_int(0), b_byte(Byte.MIN_VALUE) * p_byte(0))
    assertEquals(b_int(-128), b_byte(Byte.MIN_VALUE) * p_byte(1))
