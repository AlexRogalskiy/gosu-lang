<?xml version="1.0"?>
<class>
  <desc>Similar to a java.lang.ThreadLocal, stores data specific to the current WSI request in progress.</desc>
  <method name="get">
    <desc>Returns the current value of this request local.</desc>
    <return>the current value of this request local</return>
  </method>
  <method name="set">
    <desc>Sets a new value of this request local.</desc>
    <param name="value" type="T">the new value</param>
  </method>
</class>